{
    "name": "AeonMusicBotGo",
    "description": "Telegram Music Bot",
    "repository": "https://github.com/5hojib/AeonMusicBotGo",
    "logo": "https://cdn-icons-png.flaticon.com/512/5968/5968804.png",
    "success_url": "/",
    "env": {
        "APP_ID": {
            "description": "Telegram APP_ID from my.telegram.org",
            "required": true
        },
        "API_HASH": {
            "description": "Telegram API_HASH from my.telegram.org",
            "required": true
        },
        "USER_SESSION": {
            "description": "Telegram user session string",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Telegram bot token from @BotFather",
            "required": true
        }
    },
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "basic"
        }
    },
    "stack": "container"
}